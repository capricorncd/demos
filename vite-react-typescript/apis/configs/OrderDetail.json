{
  "fileName": "OrderDetail",
  "title": "订单详情API",
  "desc": [
    "获取订单详情"
  ],
  "apiUrl": "api/order/detail",
  "params": [
    {
      "key": "orderId",
      "type": "number",
      "desc": "订单ID，为0时返回最新订单数据"
    }
  ],
  "method": "get",
  "children": [
    {
      "name": "OrderDetailResponse",
      "desc": [
        "接口返回数据"
      ],
      "type": "interface",
      "columns": [
        {
          "key": "order_id",
          "required": true,
          "type": "string",
          "desc": "订单号"
        },
        {
          "key": "status",
          "required": true,
          "type": "number",
          "desc": "1已下单，2制作中，3已完成"
        },
        {
          "key": "food_list",
          "required": true,
          "type": "OrderDetailFoodItem[]",
          "desc": "食品列表"
        },
        {
          "key": "remark",
          "required": true,
          "type": "string",
          "desc": "下单时的备注信息"
        },
        {
          "key": "create_date",
          "required": true,
          "type": "string",
          "desc": "下单时间，时间戳还是yyyy-MM-dd hh"
        }
      ]
    },
    {
      "name": "OrderDetailFoodItem",
      "desc": [
        "订单详情食品数据"
      ],
      "type": "interface",
      "columns": [
        {
          "key": "cover",
          "required": true,
          "type": "string",
          "desc": "封面图"
        },
        {
          "key": "name",
          "required": true,
          "type": "string",
          "desc": "名称"
        },
        {
          "key": "sub_name",
          "required": true,
          "type": "string",
          "desc": "二级名称"
        },
        {
          "key": "price",
          "required": true,
          "type": "number",
          "desc": "价格"
        },
        {
          "key": "count",
          "required": true,
          "type": "number",
          "desc": "数量"
        },
        {
          "key": "specifications",
          "required": true,
          "type": "OrderDetailSpecification[]",
          "desc": "规格列表"
        }
      ]
    },
    {
      "name": "OrderDetailSpecification",
      "desc": [
        "订单详情食品规格列表元素数据"
      ],
      "type": "interface",
      "columns": [
        {
          "key": "name",
          "required": true,
          "type": "string",
          "desc": "规格名称"
        },
        {
          "key": "price_difference",
          "required": true,
          "type": "number",
          "desc": "差价"
        }
      ]
    }
  ]
}